{
  "expo": {
    "name": "Pilkada Desa",
    "slug": "pilkada-desa",
    "version": "1.0.0",
    "description": "Sistem Pemilihan Kepala Desa Digital",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "owner": "your-expo-username",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourname.pilkadadesa"
    },
    "android": {
      "package": "com.yourname.pilkadadesa",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#DC2626"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font", 
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "your-project-id-will-be-generated"
      }
    }
  }
}
